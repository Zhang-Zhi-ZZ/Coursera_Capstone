   "source": [
    "### 1.2 Get the GIS location data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 88,
   "metadata": {},
   "outputs": [],
   "source": [
    "geolocator = Nominatim(user_agent = 'My app')\n",
    "def getLat(address):\n",
    "    loc = address + ', Hamilton, Canada'\n",
    "    if geolocator.geocode(loc):\n",
    "        return geolocator.geocode(loc).latitude \n",
    "def getLon(address):\n",
    "    loc = address + ', Hamilton, Canada'\n",
    "    if geolocator.geocode(loc):\n",
    "        return geolocator.geocode(loc).longitude\n",
    "rent_df['Latitude'] = rent_df['Neighbourhood'].apply(getLat) \n",
    "rent_df['Longitude'] = rent_df['Neighbourhood'].apply(getLon) "
   ]
  },
  {
